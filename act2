#include <iostream>
#include <conio.h>
#include <iomanip>

using namespace std;
int main()
{
	double sum, average, num1, num2, num3, num4, num5;


	cout << "Enter number: ";
	cin >> num1 >> num2 >> num3 >> num4 >> num5 >>;

	sum = num1 + num2 + num3 + num4 + num5
	cout << "Sum" << sum << endl;
	
	average = sum / 5
	cout << "Average" << average << endl;

	_getch();
    return 0;
}
